{
  "actionButtons": {
    "defaultColor": "#ff0034", // Can also use string color names.
    "loadNpmCommands": false, // Disables automatic generation of actions for npm commands.
    "reloadButton": "♻️", // Custom reload button text or icon (default ↻). null value enables automatic reload on configuration change
    "homepage": "https://holykiller.github.io/AngularBasics/",
    "commands": [
      {
        "name": "Start Server",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng serve -o"
      },
      {
        "name": "Buid Div",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng build --prod --base-href https://holykiller.github.io/Resume/"
      },
      {
        "name": "Deploy To Main Resume",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng deploy --repo https://github.com/holykiller/Resume.git --base-href https://holykiller.github.io/Resume/"
      },
      {
        "name": "Deploy To Pages",
        "singleInstance": true,
        "color": "#fff",
        "command": "ngh"
      },
      {
        "name": "Buid Resume Angular",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng build --prod --base-href https://holykiller.github.io/ResumeAngular/"
      },
      {
        "name": "Start Testing",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng test"
      },
      {
        "name": "Code Coverage",
        "singleInstance": true,
        "color": "#E28032",
        "command": "ng test --code-coverage"
      },
      {
        "name": "Update Angular and Angular Cli",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng update @angular/core @angular/cli"
      },
      {
        "name": "Update Packages",
        "singleInstance": true,
        "color": "#fff",
        "command": "npm update"
      },
      {
        "name": "Buid To E:/HTML/Resume ",
        "singleInstance": true,
        "color": "#fff",
        "command": "ng build --prod --output-path=E:/HTML/Resume --delete-output-path false --base-href https://holykiller.github.io/Resume/"
      },
      {
        "name": "Start Server 4201",
        "singleInstance": true,
        "color": "#fff7",
        "command": "ng serve --port 4201 -o"
      },
      {
        "name": "Extract x18n",
        "singleInstance": true,
        "color": "#ff2f",
        "command": "ng xi18n --output-path src/locale"
      }
    ]
  }
}
